{
    "jsc": {
        "keepClassNames": true,
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": true,
            "dynamicImport": false
        },
        "target": "es2022",
        "baseUrl": ".",
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        }
    },
    "module": {
        "type": "es6",
        "strict": true
    }
}